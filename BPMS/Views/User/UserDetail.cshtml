@model UserDetailDTO
@{
    ViewData["title"] = "Detail u≈æivatele";
}
<script src="~/js/User/User.js"></script>

<div id="PageNavId" class="page-navbar">
    @await Html.PartialAsync("Partial/_UserDetailHeader", Model)
</div>
<div id="PageContentId" class="d-flex pt-3">
    <div id="DetailDivId" class="detail-div container-lg">
        @await Html.PartialAsync("Partial/_UserDetail", Model)
    </div>
    <div id="OverviewDivId" class="container-lg side-overview">
        <div class="row">
            @await Html.PartialAsync("Partial/_UserCard", (Model.SelectedUser, true))
            @foreach (UserAllDTO user in Model.OtherUsers)
            {
                @await Html.PartialAsync("Partial/_UserCard", (user, false))
            }
        </div>
    </div>
    <div class="overview-nav overview-nav-hide"></div>
</div>
