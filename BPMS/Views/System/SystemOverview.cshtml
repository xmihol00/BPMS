@model SystemOverviewDTO
@{
    ViewData["Title"] = "Syst√©my";
}
<script src="~/js/System/System.js"></script>

<div id="PageNavId" class="page-navbar">
    @await Html.PartialAsync("Partial/_SystemOverviewHeader")
</div>
<div id="PageContentId" class="d-flex pt-3">
    <div id="DetailDivId" class="d-none detail-div">
        <div class="loading-content"></div>
    </div>
    <div id="OverviewDivId" class="container-lg">
        @await Html.PartialAsync("Partial/_SystemOverview", Model.Systems)
    </div>
    <div id="OverviewNavId" class="overview-nav" onclick="FilterChanges(event, '/System/Filter')">
        @await Html.PartialAsync("Partial/_OverviewFilters", "System")
    </div>
</div>